    <ToolkitPropsDir Condition="$(ToolkitInstallDir) != '' ">$(ToolkitInstallDir)\makefile\msbuild</ToolkitPropsDir>
    <SophisRepositoryDir>
      <!--e:\git_data\sophis -->
